<script type="text/javascript">
	$(window).load(function(){
		//big slider
		$('.main-slider > ul').slider({
			auto : false, //set it to true in production
			pagination : 'buttons',
			textPagination : false,
			slideDuration : 500,
			wrapPagination : true,
			paginers: '.slider-controller .slider-nav .nav-post h3',
			onSlide : function(index, slider){
				$('.main-slider .paginationSlider .btPaginationSlider').removeClass('selected');
				$('.main-slider .paginationSlider .btPaginationSlider#link'+index)
						.addClass('selected');
				$('.link-post').fadeOut(150).delay(200);
				$('.link-post#link-post-'+index).fadeIn(150);
				var $slide = $($('.wallpaper.slide')[index]);
				var srcBackground = $slide.data('background');
				var imgBackground = new Image();
				imgBackground.src = srcBackground;
				imgBackground.onload = function() {
					$slide.css('background-image', 'url('+$slide.data('background')+')');
				};
			}
			
		});
		
		//slider community cats
		$('#slider-community-cats ul').slider({
			auto : false, 
			pagination : 'arrows',
			textPagination : false,
			nbDisplayedSlides : 5,
			slideDuration : 200,
			wrapPagination : false
		});
		//small aside horizontal slider, not a plugin yet
		var $sliderNav = $('.slider-nav > ul');
		var $sliderNavs = $('.slider-nav .nav-post');
		var $wrap = $('<div></div>');
		var navSlideHeight = $sliderNavs.height();
		$wrap.height(4 * navSlideHeight);
		$wrap.css('overflow', 'hidden');
		$sliderNav.wrap($wrap);
		var sliderNavsCount = $sliderNavs.length;
		var $sliderNavTop = 0;
		$('#slider-nav-pagin-top').click(function(){
			if($sliderNavTop < 0){
				$sliderNavTop += navSlideHeight;
				$sliderNav.animate({
					top : $sliderNavTop
				});
			}
		});

		$('#slider-nav-pagin-bottom').click(function(){
			if($sliderNavTop > - navSlideHeight * (sliderNavsCount - 4)){
				$sliderNavTop -= navSlideHeight;
				$sliderNav.animate({
					top : $sliderNavTop
				});
			}
		});
	});
</script>
            </div>
