				setInterval(function(){
					var id = $('#youtube-slider q').data('id');
					$.ajax({
						url: 'http://localhost/c2j-master/www/home/ajaxYoutubeComment/'+id,
						success: function(html) {
							$('#slider-youtube-comment')
								.fadeOut(function(){
									$(this).html(html)
									.fadeIn();
								});
									
